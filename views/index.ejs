<!--ejs stands for embedded javascript
%= %whenever you are using variables
% %while using plain html..loop..switches etc
-->
<!DOCTYPE html>

<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <% include templates/headers.ejs %>
    <h1><%= title %></h1>
    <p>Welcome to this page <%= name%> <%= points  %>></p>

    <h3><%= videodata.categoryName %></h3>
    <ul>
      <% videodata.categories.forEach(function (item) {    %>
        <li><%= item.categoryName %></li>
      <% }); %>
    </ul>



    <ol>
      <% for(var i=0; i<1; i++) { %>
          <li>i am tired! man</li>
      <% } %>
    </ol>
  </body>
</html>
